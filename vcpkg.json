{
  "name": "webrtc-remote-desktop",
  "version": "1.0.0",
  "description": "Low-latency WebRTC remote desktop with AV1 encoding",
  "dependencies": [
    "libdatachannel",
    {
      "name": "cpp-httplib",
      "features": ["openssl"]
    },
    "nlohmann-json",
    "opus",
    "openssl",
    {
      "name": "ffmpeg",
      "features": ["aom", "vpx", "nvcodec", "qsv", "amf", "avcodec", "avformat", "swscale"]
    }
  ]
}
